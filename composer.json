{
    "name": "amethyst/delivery-point",
    "type": "library",
    "keywords": ["laravel", "amethyst", "amethyst-delivery-point", "delivery-point"],
    "license": "MIT",
    "require": {
        "php": ">=7.2",
        "amethyst/core": "0.2.*",
        "amethyst/address": "0.2.*",
        "amethyst/opening-hour": "0.2.*"
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "orchestra/testbench": "*"
    },
    "autoload": {
        "psr-4" : {
            "Amethyst\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": { 
            "Amethyst\\Tests\\": "tests/" 
        }
    },
    "extra": {
        "amethyst": {
            "package": "delivery-point",
            "namespace": "Railken\\Amethyst"
        },
        "laravel": {
            "providers": [
                "Amethyst\\Providers\\DeliveryPointServiceProvider"
            ]
        }
    }
}
